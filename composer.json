{
    "name": "tightenco/overload",
    "description": "Method overloading for PHP",
    "keywords": [
        "tightenco",
        "overload"
    ],
    "homepage": "https://github.com/tightenco/overload",
    "license": "MIT",
    "authors": [
        {
            "name": "Matt Stauffer",
            "email": "matt@tighten.co",
            "homepage": "https://tighten.co",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1"
    },
    "require-dev": {
        "symfony/var-dumper": "^4.3",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Tightenco\\Overload\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tightenco\\Overload\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    }
}
